<p>
  <strong>Name:</strong>
  <%= @category.name %>
</p>
<%= @category.inspect %>
<%= @category.products.inspect %>


<strong>Products:</strong>
  <ul>
    <% @category.products.each do |product| %>
      <li>   
        <%= link_to "#{product.name}", product_path(product) %>
        
      </li>
    <%end%>
  </ul>
</p>

<%= link_to 'Edit', edit_category_path(@category) %> |
<%= link_to 'Back', categories_path %>
